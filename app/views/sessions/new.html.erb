<%= form_for :session, url: create_login_path do |f| %>
<table>
  <tr>
    <td>
     <%= f.label  :email     %>
     <br>
     <%= f.email_field :email %>
    </td>
  </tr>
  <tr>
    <td>
     <%= f.label  :password     %>
     <br>
     <%= f.password_field :password %>
    </td>
  </tr>
</table>
<br>
<%= f.submit "送出" %>
<% end %>
